@Test
public void testCreditCardPayment() {
  PaymentSystem paymentSystem = new PaymentSystem();
  PaymentMethod paymentMethodMock = mock(CreditCardPayment.class);
  paymentSystem.setPaymentMethod(paymentMethodMock);
  paymentSystem.pay(100);
  verify(paymentMethodMock).pay(100);
}

